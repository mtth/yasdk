{
  "name": "yasdk-github",
  "version": "0.1.6",
  "repository": "github:mtth/yasdk",
  "description": "Yet another TypesScript SDK generator",
  "keywords": [
    "yasdk"
  ],
  "author": "Matthieu Monsch <mtth@apache.org>",
  "license": "MIT",
  "type": "module",
  "files": [
    "lib/"
  ],
  "exports": {},
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out $(find test -name '*.gen' -o -name '*.gen.*')",
    "dist": "webpack",
    "fix": "prettier --write",
    "gen-yasdk": "yasdk -i resources/openapi.yaml -o src/yasdk.gen.ts",
    "gen-otc": "openapi -i resources/openapi.yaml -o src/otc.gen",
    "prepare": "pnpm run gen-yasdk && pnpm run gen-otc && pnpm run build",
    "test": "jest",
    "watch": "tsc -b -w src"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.23.0",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yasdk": "workspace:*"
  }
}
